varnishtest "multipart"

server s1 {
       rxreq
       expect req.http.t1 == "a, b"
       expect req.http.t2 == ""
       txresp
} -start

varnish v1 -vcl+backend {
	import std;
	import ${vmod_parseform};


	sub vcl_recv {
		std.cache_req_body(1MB);
		set req.http.t1 = parseform.get(key="a1");
		set req.http.t2 = parseform.get(key="alhkfsd1");
	}
} -start


client c1 {

	txreq -url "/" -req "POST" -hdr "Content-Type: text/plain" -body "ba1=a\r\na1=a\r\na2=b\r\nzz=a\r\nc\r\na1=b"
	rxresp
}

client c1 -run

